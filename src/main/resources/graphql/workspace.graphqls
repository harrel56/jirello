extend type Query {
    workspaceById(id: ID!): Workspace
    workspaceGetAll: [Workspace!]!
}

extend type Mutation {
    workspaceCreate(workspace: WorkspaceInput!): Workspace!
}

type Workspace {
    id: ID!
    title: String
    description: String
    columns: [Column!]!
}

input WorkspaceInput {
    title: String!
    description: String
}
